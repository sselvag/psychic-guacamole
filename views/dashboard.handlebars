<div class="title">
    <h1>Dashboard</h1>
</div>

<div>
    <div class="card recipe-list">
        <div class="card-body">
            <div>
                {{#if recipes.length}}
                    
                {{#each recipes as |recipe|}}
                    
                <div class="recipe col-md-5">
                    <p>{{recipe.id}}.{{recipe.title}}</p>
                </div>
                        
                {{/each}}
                    
                {{else}}
                <p class="align-text-center">No current recipes, get a new recipe!</p>
                    
                {{/if}}
            </div>
        </div>
    </div>
</div>






